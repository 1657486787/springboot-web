<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>JackServlet</servlet-name>
  	<display-name>JackServlet</display-name>
  	<description></description>
  	<servlet-class>com.zhuguang.jack.servlet.JackServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RoyServlet</servlet-name>
  	<display-name>RoyServlet</display-name>
  	<description></description>
  	<servlet-class>com.zhuguang.jack.servlet.RoyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JackServlet</servlet-name>
  	<url-pattern>/JackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RoyServlet</servlet-name>
  	<url-pattern>/RoyServlet</url-pattern>
  </servlet-mapping>
</web-app>
